<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>

<ScrollPane fx:id="scrollPane" hbarPolicy="NEVER" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.usb.controller.ConfigurationController">
   <content>
      <VBox fx:id="container" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity">
          <children>
              <HBox alignment="CENTER" prefHeight="100.0" prefWidth="667.0" spacing="20.0">
                  <children>
                  <ToggleButton fx:id="configuration" alignment="CENTER_LEFT" mnemonicParsing="false" onAction="#loadConfiguration" text="Configuration Setting" />
                  <ToggleButton fx:id="ssid" mnemonicParsing="false" onAction="#loadSSID" prefHeight="26.0" prefWidth="170.0" text="SSID" />
                  <ToggleButton fx:id="communication" mnemonicParsing="false" onAction="#loadCommunication" text="Communication Setting" />
                  </children>
              </HBox>
            <VBox prefHeight="200.0" prefWidth="100.0" />
          </children>
      </VBox>
   </content>
</ScrollPane>
