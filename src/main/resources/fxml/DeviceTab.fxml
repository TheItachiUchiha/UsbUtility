<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.usb.controller.DeviceTabController">
   <children>
      <Label alignment="CENTER" text="Device Name : Something">
         <VBox.margin>
            <Insets bottom="10.0" left="100.0" right="10.0" top="10.0" />
         </VBox.margin>
      </Label>
      <TabPane fx:id="tabPane" minHeight="-Infinity" minWidth="-Infinity" side="LEFT" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
         <tabs>
            <Tab fx:id="configurationTab" text="CONFIGURATION">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="344.0" prefWidth="311.0" />
               </content>
            </Tab>
            <Tab fx:id="calibrationTab" text="CALIBRATION">
               <content>
                  <VBox alignment="CENTER">
                     <children>
                        <GridPane alignment="CENTER" hgap="10.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="167.0" prefWidth="433.0" vgap="10.0">
                           <columnConstraints>
                              <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                              <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="157.0" minWidth="10.0" prefWidth="116.0" />
                              <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="130.0" minWidth="10.0" prefWidth="130.0" />
                           </columnConstraints>
                           <rowConstraints>
                              <RowConstraints maxHeight="124.0" minHeight="0.0" prefHeight="27.0" valignment="CENTER" vgrow="SOMETIMES" />
                              <RowConstraints maxHeight="50.0" minHeight="0.0" prefHeight="50.0" vgrow="SOMETIMES" />
                              <RowConstraints maxHeight="50.0" minHeight="0.0" prefHeight="50.0" vgrow="SOMETIMES" />
                           </rowConstraints>
                           <children>
                              <Label text="Soak Time" />
                              <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="16.0" prefWidth="150.0" text="Environment Value" GridPane.columnIndex="1" />
                              <Label text="Sensor Value" GridPane.columnIndex="2" />
                              <TextField alignment="CENTER" prefHeight="45.0" prefWidth="200.0" text="20:00" GridPane.rowIndex="1" />
                              <TextField alignment="CENTER" prefHeight="45.0" prefWidth="200.0" text="22 C" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                              <TextField alignment="CENTER" editable="false" prefHeight="44.0" prefWidth="200.0" text="22.2 C" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                              <Button mnemonicParsing="false" prefHeight="26.0" prefWidth="147.0" text="Submit" GridPane.columnSpan="3" GridPane.rowIndex="2" />
                           </children>
                           <padding>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </padding>
                        </GridPane>
                     </children>
                  </VBox>
               </content>
            </Tab>
            <Tab fx:id="dataDownloadTab" text="DATA DOWNLOAD">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
               </content>
            </Tab>
         </tabs>
      </TabPane>
   </children>
</VBox>
