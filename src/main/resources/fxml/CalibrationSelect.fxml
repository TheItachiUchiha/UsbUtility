<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="TOP_CENTER" spacing="20" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.usb.controller.CalibrationSelectController">
    <children>
        <Label alignment="CENTER" contentDisplay="TOP" text="Please Select the Parameter" />
        <HBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="76.0" prefWidth="600.0" spacing="20.0">
            <children>
                <Button fx:id="humidity" mnemonicParsing="false" onAction="#openSettings">
                    <graphic>
                        <ImageView fitHeight="62.0" fitWidth="53.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../images/humidity.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="temperature" mnemonicParsing="false" onAction="#openSettings" prefHeight="53.0" prefWidth="64.0">
                    <graphic>
                        <ImageView fitHeight="54.0" fitWidth="59.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../images/thermometer.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="100.0" />
    </padding>
</VBox>
