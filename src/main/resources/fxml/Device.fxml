<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<TitledPane fx:id="titledPane" expanded="false" text="Device: 12345678abcdefgh&#10;&#10;Communication Medium: Ethernet" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.usb.controller.DeviceController">
    <content>
        <GridPane hgap="10.0" prefHeight="100.0" prefWidth="598.0" vgap="10.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Label fx:id="humidity" text="Humidity : 60%" />
                <Label fx:id="temperature" text="Temperature : 45 C" GridPane.columnIndex="1" />
                <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0" GridPane.columnSpan="2" GridPane.rowIndex="1">
                    <children>
                        <Button fx:id="configuration" mnemonicParsing="false" onAction="#openConfiguration" text="CONFIGURATION" />
                        <Button fx:id="calibration" mnemonicParsing="false" onAction="#openCalibration" text="CALIBRATION" />
                        <Button fx:id="dataDownload" mnemonicParsing="false" onAction="#openDataDownload" text="DATADOWNLOAD" />
                    </children>
                </HBox>
            </children>
        </GridPane>
    </content>
</TitledPane>
